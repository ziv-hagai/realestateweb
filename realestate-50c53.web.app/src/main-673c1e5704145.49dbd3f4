@import url(https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800;900&display=swap);
:root {
  --primary: #00a9d6;
}
.startforfree-btn {
  padding: 8px 22px;
  border-radius: 10px;
  outline: none;
  border: none;
  font-size: 15px;
  color: #fff;
  cursor: pointer;
  background-color: #00a9d6;
  background-color: var(--primary);
  font-weight: 500;
}

.login-btn {
  padding: 6px 22px; 
  border-radius: 10px;
  outline: none;
  border: none;
  font-size: 15px;
  color: #444652;
  cursor: pointer;
  background-color: #fff;
  font-weight: 500;
}

::marker{
  display: none;
}
.mainHeader {
  padding: 10px 20px;
  border-bottom: 1px solid #00000033;
  background-color: #fff;
  height: 64px;
  display: flex;
  align-items: center;
  position: relative;
}
.navbar {
  display: flex;
  align-items: center;
  width: 100%;
  justify-content: space-between;
}
.navbar-logo {
  cursor: pointer;
  width: 135px;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}
.mainHeader_Menu,
.rightSideBtns {
  display: flex;
  align-items: center;
}
.rightSideBtns ul{
  display: flex;
  align-items: center;
  list-style-type: none;
}
.fa-firstdraft {
  margin-left: 0.5rem;
  font-size: 1.6rem;
}
.nav-menu {
  display: flex;
  align-items: center;
  margin-left: 35px;
}
.nav-item {
  display: flex;
  align-items: center;
}
.nav-links {
  text-decoration: none;
  padding: 8px 10px;
  font-size: 16px;
  line-height: 24px;
  color: #39455380;
  font-weight: 500;
}
.nav-links.active {
  color: #00A9D6;
}
.iconMenu {
  height: 30px;
  width: 30px;
  border-radius: 10px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 5px;
}
.iconMenu--btn {
  box-shadow: 0px 1px 8px 0px rgb(0 0 0 / 4%);
  height: 40px;
  width: 40px;
  position: relative;
}
.newAdd {
  position: relative;
}
.newAdd::after {
  content: '';
  height: 6px;
  width: 6px;
  border-radius: 50px;
  background-color: #ff0000;
  display: block;
  position: absolute;
  top: 8px;
  right: 8px;
}
.iconMenu svg {
  color: #000;
  fill: #000;
  height: 20px;
  width: 20px;
}
.fa-bars {
  color: rgb(0, 0, 0);
}
.nav-links-mobile {
  display: none;
}
.menu-icon {
  display: none;
}
.d-lg-block {
  display: block;
}
.navIcons svg {
  width: 28px;
  height: 28px;
}
.navIcons span {
  display: flex;
  align-items: center;
  justify-content: center;
}
.navIcons.open .openIcon {
  display: flex;
}
.navIcons.open .closeIcon {
  display: none;
}
.navIcons.close .openIcon {
  display: none;
}
.navIcons.close .closeIcon {
  display: flex;
}
.NavbarItem .buttons .dropdown {
  max-width: 370px;
  box-shadow: 0px 2px 8px 0px rgb(0 0 0 / 12%);
  background-color: white;
  border: #f1f1f1 solid 1pt;
  position: absolute;
  z-index: 1001;
  border-radius: 10pt;
  padding: 15px 22px;
  top: 60px;
  align-items: center;
}
.NavbarItem .buttons .dropdown select {
  border: 1px solid #ddd;
  border-radius: 10px;
  padding: 10px 15px;
  width: 200px;
  height: auto;
}

@media screen and (max-width: 991px) {
  .d-lg-block {
    display: none;
  }
  .iconMenu {
    height: 25px;
    width: 25px;
  }
  .iconMenu--btn {
    height: 35px;
    width: 35px;
  }
  .navbar-logo {
    width: 85px;
  }
  .NavbarItems {
    position: relative;
  }
  .nav-menu {
    display: block;
    width: 100%;
    position: absolute;
    top: 64px;
    left: 100%;
    opacity: 1;
    transition: all 0.5s ease;
    margin-left: 0;
  }
  .nav-menu.active {
    background: #fff;
    left: 0;
    opacity: 1;
    transition: all 0.5s ease;
    z-index: 12;
    box-shadow: 0px 2px 5px 0px rgb(0 0 0 / 12%);
  }
  .nav-links {
    text-align: left;
    padding: 10px 25px;
    display: block;
    width: 100%;
  }
  .menu-icon {
    display: block;
    cursor: pointer;
    margin-left: 10px;
  }
  .fa-times {
    color: #fff;
    font-size: 2rem;
  }
  .nav-links-mobile {
    display: block;
    text-align: center;
    padding: 0;
    margin: 20px 25px;
    border-radius: 4px;
    width: auto;
    background: #1888ff;
    text-decoration: none;
    color: #fff;
    font-size: 1.5rem;
    border: 1px solid #1888ff;
  }
  .nav-links-mobile a {
    color: #fff;
    text-align: center;
  }
  button {
    flex-direction: column;
  }
}
