@media screen and (max-width: 991px) { 
  .left {
    width: 100%;
    padding: 25px 15px 0;
    flex-grow: 0;
    flex-shrink: 0;
    flex-basis: 100%;
  }
  .right {
    /* width: calc(100% - 370px); */
    width: 100%;
  }
  .right {
    /* overflow: hidden; */
    transition: all 0.5s ease;
    position: absolute;
    z-index: 10;
    background: #fff;
    left: 100%;
    width: 100%;
  }
  .flex.activeTabs .right {
    left: 0px;
  }
  .mobilePageTabs {
    background: #fff;
    position: absolute;
    z-index: 11;
    border-radius: 6px;
    bottom: 100px;
    margin: 0 auto;
    align-items: center;
    justify-content: center;
    box-shadow: 0px 0px 15px 0px rgb(0 0 0 / 50%);
    cursor: pointer;
    transition: all 0.5s ease;
    display: flex;
    left: 0;
    right: 0;
    width: 300px;
  }
  .mobilePageTabs a {
    padding: 10px 12px;
    display: flex;
    align-items: center;
    justify-content: center;
    border-right: 1px solid #ddd;
    font-size: 16px;
    color: #444;
    font-weight: 500;
    min-width: 150px;
  }
  .mobilePageTabs a:last-child {
    border-right: none;
  }
  .mobilePageTabs a svg {
    width: 18px;
    height: 18px;
    margin-right: 5px;
  }
}
